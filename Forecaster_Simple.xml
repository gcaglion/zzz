#These are ignored

#Empty lines are ignored, too

<Forecaster>
	MyList = abc, def, ghijkl, mnopqras
	<Debugger>
		Verbose = false
		ScreenOutput  = true
		FileOutput	= true
		OutFilePath  = C:\temp\logs
	</Debugger>
	<Data>
		<Shape>
			SampleLen		= 20
			PredictionLen	= 3
			FeaturesCount	= 4
		</Shape>
		<Train>
			Do	= true
			<DataSet>
				DumpFileFullName = c:\temp\trainDS.dump
				<Debugger>
					Verbose = false
					ScreenOutput  = true
					FileOutput	= true
					OutFilePath  = C:\temp\logs
				</Debugger>
				<TimeSerie>
					DumpFileFullName = c:\temp\trainTS.dump
					Date0				= 201612300000
					HistoryLen			= 503
					DataTransformation	= DT_DELTA
					BWCalc				= true
					StatisticalFeatures	= TSF_HISTVOL, TSF_KURTOSIS, TSF_MEAN
					<FXDB_DataSource>
						Symbol				= EURUSD
						TimeFrame			= H1
						IsFilled			= false
						<DBConnection>
							UserName	= History
							Password	= HistoryPwd
							ConnString	= Algo
						</DBConnection>
					</FXDB_DataSource>
				</TimeSerie>
				BatchSamplesCount	= 10
				SelectedFeatures	= FXOPEN, FXHIGH, FXLOW, FXCLOSE
				BWFeatures			= 1,2
			</DataSet>
		</Train>
		<Validation>
			Do	= false
			<DataSet>
				<TimeSerie>
					<File_DataSource>
						FileName = C:/temp/SunSpots.txt
						FieldSep = TAB_SEPARATOR
						CalcBW = true
						BWfeatureH = 4
						BWfeatureL = 5
					</File_DataSource>
				</TimeSerie>
			</DataSet>
		</Validation>
		<Test>
			Do	= false
			<DataSet>
				<TimeSerie>
					Date0				= 201512300000
					HistoryLen			= 105
					DataTransformation	= DT_DELTALOG
					BWCalc				= true
					StatisticalFeatures	= TSF_HISTVOL, TSF_KURTOSIS, TSF_MEAN
					<FXDB_DataSource>
						Symbol				= EURUSD
						TimeFrame			= H1
						IsFilled			= false
						<DBConnection>
							UserName	= History
							Password	= HistoryPwd
							ConnString	= Algo
						</DBConnection>
					</FXDB_DataSource>
				</TimeSerie>
				BatchSamplesCount	= 50
				SelectedFeatures	= FXOPEN, FXHIGH, FXLOW, FXCLOSE
				BWFeatures			= 1,2
			</DataSet>
		</Test>
	</Data>
	<Engine>
		<Debugger>
			Verbose 		= true
			Timing			= false
			PauseOnError	= true
			Dest			= DBG_DEST_BOTH
			OutFileFullName	= C:\temp\logs\mainDebugger.log
		</Debugger>		
		Type 		= ENGINE_CUSTOM
		<Custom>
			CoresCount	= 1
			<Core0>
				<Layout>
					Type	= CORE_NN
					Parents			= 
					ParentsConnType	= 
				</Layout>
				<Parameters>
					<Topology>
						LevelRatio			= 0.5
						LevelActivation		= NN_ACTIVATION_TANH, NN_ACTIVATION_TANH, NN_ACTIVATION_TANH
						UseContext			= true
						UseBias				= false
					</Topology>
					<Training>
						MaxEpochs			= 50
						TargetMSE			= 0.0001
						NetSaveFrequency	= 10
						StopOnDivergence	= true
						BP_Algo				= BP_STD
						<BP_Std>
							LearningRate		= 0.001
							LearningMomentum	= 0.4
						</BP_Std>
						<BP_Qing>
						</BP_Qing>
						<BP_Qprop>
						</BP_Qprop>
						<BP_RProp>
						</BP_RProp>
					</Training>
				</Parameters>
			</Core0>
		</Custom>
		<XIE>
		</XIE>
		<WNN>
		</WNN>
	</Engine>
	<Persistor>
		saveNothing		= false		
		saveClient		= true
		saveMSE			= true
		saveRun			= true
		saveInternals	= false
		saveImage		= true
		saveToDB		= true
		saveToFiles		= true
		<DestDB>
			UserName	= CULogUser
			Password	= LogPwd
			ConnString	= Algo
		</DestDB>
		<DestFiles>
			Client		= c:\temp\logs\Persistor\Client.log
			MSE			= c:\temp\logs\Persistor\MSE.log
			Run			= c:\temp\logs\Persistor\Run.log
			Internals	= c:\temp\logs\Persistor\Internals.log
			Image		= c:\temp\logs\Persistor\Image.log
		</DestFiles>
	</Persistor>
</Forecaster>
