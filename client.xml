			<Debugger>
				Verbose = false
				ScreenOutput  = true
				FileOutput	= true
				OutFilePath  = C:\temp\logs
			</Debugger>
			Do	= true
			<DataSet>
				<TimeSerie>
					Date0				= 201712300000
					HistoryLen			= 505
					DataTransformation	= DT_DELTALOG
					BWCalc				= true
					StatisticalFeatures	= TSF_HISTVOL, TSF_KURTOSIS, TSF_MEAN
					<FXDB_DataSource>
						Symbol				= EURUSD
						TimeFrame			= H1
						IsFilled			= false
						<DBConnection>
							UserName	= History
							Password	= HistoryPwd
							ConnString	= Algo
						</DBConnection>
					</FXDB_DataSource>
				</TimeSerie>
				BatchSamplesCount	= 50
				SelectedFeatures	= FXOPEN, FXHIGH, FXLOW, FXCLOSE
				BWFeatures			= 1,2
			</DataSet>
		</Train>
		<Validation>
			Do	= false
		</Validation>
		<Test>
			Do	= true
			<DataSet>
				<TimeSerie>
					Date0				= 201612300000abc
					HistoryLen			= 100
					DataTransformation	= DT_DELTALOG
					BWCalc				= true
					StatisticalFeatures	= TSF_HISTVOL, TSF_KURTOSIS, TSF_MEAN
					<File_DataSource>
						FileName = C:/temp/SunSpots.txt
						FieldSep = TAB_SEPARATOR
						CalcBW = true
						BWfeatureH = 4
						BWfeatureL = 5
					</File_DataSource>
				</TimeSerie>
				BatchSamplesCount	= 50
				SelectedFeatures	= FXOPEN, FXHIGH, FXLOW, FXCLOSE
				BWFeatures			= 1,2
			</DataSet>
		</Test>
	</Data>
