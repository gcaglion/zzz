					UseBias				= false
				</Topology>
				<Training>
					MaxEpochs			= 50
					TargetMSE			= 0.0001
					NetSaveFrequency	= 10
					StopOnDivergence	= true
					BP_Algo				= BP_STD
					<BP_Std>
						LearningRate		= 0.01
						LearningMomentum	= 0.4
