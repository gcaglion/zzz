<Forecaster>
	<Debugger>
		Verbose = false
		ScreenOutput  = true
		FileOutput	= true
		OutFilePath  = C:\temp\logs
	</Debugger>
	<Data>
		<Shape>
			SampleLen		= 100
			PredictionLen	= 5
			FeaturesCount	= 4
		</Shape>
